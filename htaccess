
# DO NOT REMOVE THIS LINE AND THE LINES BELLOW SSL_REDIRECT:runalotus.com
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} (www\.)?runalotus.com
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
